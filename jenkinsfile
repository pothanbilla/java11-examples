node('jdk11-maven') {
    // some block
    stage('git') {
    // some block
    git 'https://github.com/pothanbilla/java11-examples.git'
    }
    stage('build') {
        sh '''
           echo "PATH=${PATH}"
           echo "M2_HOME=${M2_HOME}"
           
        '''
        sh 'mvn clean package'
    }
    stage('archive') {
        archive 'target/*.jar'
    }
}
